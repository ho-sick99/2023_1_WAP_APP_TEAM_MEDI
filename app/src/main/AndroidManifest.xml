<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.mediproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MediApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.MediProject"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.MediProject">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>

            <nav-graph android:value="@navigation/home_nav" />
            <nav-graph android:value="@navigation/community_nav" />
            <nav-graph android:value="@navigation/intro_nav" />
            <nav-graph android:value="@navigation/mypage_nav" />
            <nav-graph android:value="@navigation/setting_nav" />
            <nav-graph android:value="@navigation/search_medicines_nav" />
            <nav-graph android:value="@navigation/news_nav" />
        </activity>
    </application>

</manifest>